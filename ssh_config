Host *
  AddKeysToAgent yes
  UseKeychain yes
  IdentityFile ~/.ssh/id_rsa

Host ops-bastion
  Hostname ops-inf-ssh-use1.blackfynn.io
  User scirner
  Port 22
  StrictHostKeyChecking no
  PasswordAuthentication no
  ForwardAgent yes
  ServerAliveInterval 30
  ServerAliveCountMax 3

